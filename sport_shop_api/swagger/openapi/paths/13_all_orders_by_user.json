{
    "/api/v1/orders/": {
        "get": {
            "description": "Show num of orders and Inf",
            "tags": [
                "Order"
            ],
            "responses": {
                "200": {
                    "$ref": "#/components/responses/AllOrders"
                },
                "401": {
                    "description": "Returned after sending invalid set of credentials",
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "errors": {
                                        "type": "object",
                                        "example": {
                                            "detail": [
                                                "Authentication credentials were not provided."
                                            ]
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }

}